public class Fibo {
    public static void main(String[] args) {
        int n = 21; // Number of terms in the Fibonacci series
        int a = 0, b = 1;

        System.out.print("Fibonacci Series up to " + n + " terms: ");

        for (int i = 0; i < n; i++) {
            System.out.print(a + " ");
            int next = a + b;
            a = b;
            b = next;
        }
    }
}
